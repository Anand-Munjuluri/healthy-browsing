{
    "manifest_version": 3,
    "name": "Health Reminder",
    "version": "1.0",
    "description": "Reminds you to take breaks and stay hydrated!",
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.jpg",
        "48": "images/icon48.jpg",
        "128": "images/icon128.jpg"
      }
    },
    "permissions": ["alarms","activeTab","storage", "notifications"],
    "icons": {
      "16": "images/icon16.jpg",
      "48": "images/icon48.jpg",
      "128": "images/icon128.jpg"
    }
  }
  
